<VirtualHost *:8080>
	ServerAdmin webmaster@localhost
	ServerName www.1tian.net:8080
	DocumentRoot /data/mov1
	
	#CBandSpeed 4096 10 300
	# Maximal 10kB/s speed, 3 requests/s and 2 open connections for any remote client
	#CBandRemoteSpeed 100kb/s 10 10
	
	
	<Directory /data/mov/>
		Options Indexes FollowSymLinks MultiViews
		AllowOverride None
		EnableSendfile Off
		Order allow,deny
		allow from all
		# Uncomment this directive is you want to see apache2's
		# default start page (in /apache2-default) when you go to /
		#RedirectMatch ^/$ /apache2-default/
	</Directory>
#	alias /j "/data/jiao"
	<Directory "/data/jiao">
		Options Indexes MultiViews
		AllowOverride None
	</Directory>
	ServerSignature On
</VirtualHost>	

<VirtualHost *:80>
	ServerName down.1tian.net
	DirectoryIndex index.php
	DocumentRoot /data/yitian/myyitian/media
	<Directory /data/yitian/myyitian/media>
		Order allow,deny
		allow from all
		Options None
		AllowOverride None
	</Directory>
	

	
</VirtualHost>




<VirtualHost *:443>
	ServerName down.1tian.net
	DirectoryIndex index.php
#	DocumentRoot /data/yitian/myyitian/media
	
	SSLEngine on
	SSLCipherSuite ALL:!ADH:!EXPORT56:RC4+RSA:+HIGH:+MEDIUM:+LOW:+SSLv2:+EXP:+eNULL
	SSLCertificateKeyFile conf/down.1tian.pem
	SSLCertificateFile conf/down.1tian.crt

	Alias /awstatsclasses "/usr/local/awstats/wwwroot/classes/"
	Alias /awstatscss "/usr/local/awstats/wwwroot/css/"
	Alias /awstatsicons "/usr/local/awstats/wwwroot/icon/" 
	
	Alias /kk "/usr/local/awstats/wwwroot/cgi-bin"
	
	Alias /admindb "/data/xuke/phpmyadmin"
    <Directory "/data/xuke/phpmyadmin/">
		Order allow,deny
		allow from all
		Options None
		AllowOverride None
    </Directory>

	<directory "/usr/local/awstats/wwwroot/cgi-bin">
		AllowOverride None
		SetHandler perl-script
		PerlResponseHandler ModPerl::Registry
		PerlOptions +ParseHeaders
		
		Options ExecCGI -MultiViews +SymLinksIfOwnerMatch
		Order allow,deny
		Allow from all
		AuthType Basic
		AuthName "需要认证"
		AuthBasicProvider file
		AuthUserFile /etc/apache2/dav_svn.passwd
		Require user xuke
	</directory>
	
</VirtualHost>


